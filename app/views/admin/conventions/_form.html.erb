<%= form_with model: [:admin, @convention] do |f| %>
  <div class="row g-5">
    <div class="col-4">
      <%= f.label :year %>
      <%= f.text_field :year, class: 'form-control' %>
    </div>
    <div class="col-4">
      <%= f.label :start_date %>
      <%= f.date_field :start_date, class: 'form-control' %>
    </div>
    <div class="col-4">
      <%= f.label :end_date %>
      <%= f.date_field :end_date, class: 'form-control' %>
    </div>
  </div>
  <div class="row g-5">
 
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>